<h1>Lista</h1>
<hr>
    {{#each items}}
        <div class="mb-4">
            <h4>{{nome}}</h4>
            <span>Slug: {{slug}}</span>
            <br>
            <span>Data: {{data}}</span>
            <br>
            <a href="/admin/lista/edit/{{_id}}">
                <button class="btn btn-success mt-2">Editar</button>
            </a>
        </div>
    {{else}}

    {{/each}}
<a href="/admin/lista/add"><button class="btn btn-success">Adicionar</button></a>